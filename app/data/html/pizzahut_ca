       window.menu_tag = \"SIDES\";              } else if (sHash.indexOf(\"#drinks\") != -1) {                  intMenuCategoryID = 5;                  sHashRoot = \"#drinks\";                  window.menu_tag = \"DRINKS\";              }                var bChangeCategory = false;              if (intMenuCategoryID > 0) {                  if (!($('#ctl00_ContentPlaceHolder1_hdnMenuCategoryID').val() == intMenuCategoryID)) {                      bChangeCategory = true;                  }              }                if (bChangeCategory == false) {                  if (sHash.indexOf(\"-id-\") != -1) {                      ShowMenuItemDetails('divMenuItemDetails-' + sHash.substr(sHash.indexOf(\"-id-\") + 4));                  } else {                      ShowMenuItemDetails('divMenuItemDetails-' + $(\"#ctl00_ContentPlaceHolder1_hdnDefaultMenuItemID\").val());                  }              } else {                  if (sHash.indexOf(\"-id-\") != -1) {                      $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTag\").val(sHashRoot);                      $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTagID\").val(sHash.substr(sHash.indexOf(\"-id-\") + 4));                  }                  switch (intMenuCategoryID) {                      case 1:                          showPizzaMenu();                          break;                      case 2:                          showWingsMenu();                          break;                      case 3:                          showPastaMenu();                          break;                      case 4:                          showSidesMenu();                          break;                      case 5:                          showDrinksMenu();                          break;                  }              }          }            function openUtensils() {              $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTagID\").val(-1);              $('#fork').animate({ left: '-=250' }, 300, 'easeOutCubic',                  function () {                      // Animation complete.                  });              $('#knife').animate({ right: '-=250' }, 300, 'easeOutCubic',                  function () {                      //Animation complete.                      checkMainHashTag();                  });          }            function openUtensilsSimple() {              $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTagID\").val(-1);              $('#fork').animate({ left: '-=250' }, 300, 'easeOutCubic',                  function () {                      // Animation complete.                  });              $('#knife').animate({ right: '-=250' }, 300, 'easeOutCubic',                  function () {                      //Animation complete.                      skipper2();                      //foodCycleShortcut();                  });          }            function checkMainHashTag() {              var intOpenMode = 0;              try {                  var sHash = \"\";                  sHash = window.location.hash.toLowerCase();                  if (sHash.indexOf(\"#shortcut\") != -1) {                      intOpenMode = -1;                  } else if (sHash.indexOf(\"#pizza\") != -1) {                      intOpenMode = 1;                  } else if (sHash.indexOf(\"#wings\") != -1) {                      intOpenMode = 2;                  } else if (sHash.indexOf(\"#pasta\") != -1) {                      intOpenMode = 3;                  } else if (sHash.indexOf(\"#sides\") != -1) {                      intOpenMode = 4;                  } else if (sHash.indexOf(\"#drinks\") != -1) {                      intOpenMode = 5;                  }              } catch (e) {                  //do nothing              }                var t;              if (intOpenMode == -1) {                  t = setTimeout(\"foodCycleShortcut()\", 800);              } else if (intOpenMode == 1) {                  t = setTimeout(\"menuCategoryShortcut(1)\", 800);              } else if (intOp