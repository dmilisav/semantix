ument.getElementById(\"menunav\").getElementsByTagName(\"DIV\");              for (var i = 0; i < arrInnerDivs.length; i++) {                  intCategoryID = $('#' + arrInnerDivs[i].id).attr(\"MenuCategoryID\");                    oCurrentDiv = arrInnerDivs[i];                  arrInnerImages = oCurrentDiv.getElementsByTagName(\"IMG\");                  if (arrInnerImages[0].src.indexOf('-on') >= 0) {                      if (!(intMenuCategoryID == intCategoryID)) {                          arrInnerImages[0].src = arrInnerImages[0].src.replace('-on', '-off');                          $('#' + oCurrentDiv.id).animate({ top: '-=19' }, 200);                      }                  } else {                      if (intMenuCategoryID == intCategoryID) {                          arrInnerImages[0].src = arrInnerImages[0].src.replace('-off', '-on');                          $('#' + oCurrentDiv.id).animate({ top: '+=19' }, 200);                      }                  }              }          }            function rollerMenuItem(pObj) {              if ($('#' + pObj.id).hasClass('MenuItemSmallTitle-Red')) {                  $('#' + pObj.id).removeClass('MenuItemSmallTitle-Red');                  $('#' + pObj.id).addClass('MenuItemSmallTitle-White');                  $('#' + pObj.id).find('.MenuItemDetailsBtn').show();                    if ($('#ctl00_ContentPlaceHolder1_hdnMenuCategoryID').val() == 5) {                      //Drink Images have varying widths. Need to place button at dynamic position.                      var intPos = (($('#' + pObj.id).width() / 2) - 104);                      $('#' + pObj.id).find('.MenuItemDetailsBtn').css({ left: intPos });                  }              } else {                  $('#' + pObj.id).removeClass('MenuItemSmallTitle-White');                  $('#' + pObj.id).addClass('MenuItemSmallTitle-Red');                  $('#' + pObj.id).find('.MenuItemDetailsBtn').hide();              }          }            function ShowMenuItemDetai