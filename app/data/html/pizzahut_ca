            window.location.hash = \"#sides\" + strHashTagEnd;                      jUpdateInProgress = false;                  });              } else if (intMenuCategoryID == 5) {   //Drinks                  $('#divMenuItems').show();                  $('#divMenuItems').animate({ top: '94' }, 500, function () {                      $('#menu-intro').animate({ height: '500' }, 500);                      window.location.hash = \"#drinks\";                      jUpdateInProgress = false;                  });              } else {                  jUpdateInProgress = false;              }          }            function skipper() {              jSkipClicked = true;              $('#sides').hide();              $('#pasta').hide();              $('#pizza').hide();              $('#wings').hide();              $('#dessert').hide();              $('#explore').show();              openNav();              $('#btnSkip').hide();          }            function skipper2() {              jSkipClicked = true;              $('#sides').hide();              $('#pasta').hide();              $('#pizza').hide();              $('#wings').hide();              $('#dessert').hide();              $('#explore').hide();              openNav2();              $('#btnSkip').hide();          }      </script>        <script type=\"text/javascript\">            var _gaq = _gaq || [];          _gaq.push(['_setAccount', 'UA-981136-5']);          _gaq.push(['_setDomainName', 'pizzahut.ca']);          _gaq.push(['_trackPageview']);            (function () {              var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;              ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';              var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);          })();        </script>    <!-- ZAP Lightning Bolt Begins -->      <script type=\"text/javascript\">      //<![CDATA[  