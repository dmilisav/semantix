       window.menu_tag = \"SIDES\";              } else if (sHash.indexOf(\"#drinks\") != -1) {                  intMenuCategoryID = 5;                  sHashRoot = \"#drinks\";                  window.menu_tag = \"DRINKS\";              }                var bChangeCategory = false;              if (intMenuCategoryID > 0) {                  if (!($('#ctl00_ContentPlaceHolder1_hdnMenuCategoryID').val() == intMenuCategoryID)) {                      bChangeCategory = true;                  }              }                if (bChangeCategory == false) {                  if (sHash.indexOf(\"-id-\") != -1) {                      ShowMenuItemDetails('divMenuItemDetails-' + sHash.substr(sHash.indexOf(\"-id-\") + 4));                  } else {                      ShowMenuItemDetails('divMenuItemDetails-' + $(\"#ctl00_ContentPlaceHolder1_hdnDefaultMenuItemID\").val());                  }              } else {                  if (sHash.indexOf(\"-id-\") != -1) {                      $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTag\").val(sHashRoot);                      $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTagID\").val(sHash.substr(sHash.indexOf(\"-id-\") + 4));                  }                  switch (intMenuCategoryID) {                      case 1:                          showPizzaMenu();                          break;                      case 2:                          showWingsMenu();                          break;                      case 3:                          showPastaMenu();                          break;                      case 4:                          showSidesMenu();                          break;                      case 5:                          showDrinksMenu();                          break;                  }              }          }            function openUtensils() {              $(\"#ctl00_ContentPlaceHolder1_hdnCurrentHashTagID\").val(-1);              $('#fork').animate({ left: '-=250' }, 300, 'easeOutCubic',          