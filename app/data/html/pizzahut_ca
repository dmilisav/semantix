on scrollerLeft() {          if (scrolling) {              return false;          }          scrolling = true;                var jObj = $('#scroll'+current);          jObj.animate({ left: '-=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+currentCheck(current+1));          jObj.animate({ left: '-=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+currentCheck(current+2));          jObj.animate({ left: '-=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+currentCheck(current+3));          jObj.css({left:306*3});          jObj.animate({ left: '-=306' }, 500, function() {              //finished              scrolling = false;              current = currentCheck(current + 1);          });          return false;      }        function scrollerRight() {          if (scrolling) {              return false;          }          scrolling = true;                var jObj = $('#scroll'+currentCheck(current-1));          jObj.css({left: -306});          jObj.animate({ left: '+=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+current);          jObj.animate({ left: '+=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+currentCheck(current+1));          jObj.animate({ left: '+=306' }, 500, function() {              //finished          });          jObj = $('#scroll'+currentCheck(current+2));          jObj.animate({ left: '+=306' }, 500, function() {              //finished              scrolling = false;              current = currentCheck(current - 1);          });          return false;      }        function currentCheck(pNum) {          if (pNum < 0) {              return gMax + pNum + 1;          }          else if (pNum > gMax) {              return (pNum - 1 - gMax);          }          else {              return pNum;          }      }  </script>  <div id=\"mainscrolle